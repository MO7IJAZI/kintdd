{
  "name": "kint-website",
  "description": "Next.js (App Router) website for KINT",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "nextjs",
    "react",
    "prisma",
    "mysql"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/fix-permissions.js && prisma generate && prisma migrate deploy && prisma db seed && cross-env NODE_OPTIONS=--max-old-space-size=4096 next build && node scripts/post-build.js",
    "start": "node server.js",
    "lint": "eslint .",
    "hostinger:setup": "npx prisma generate",
    "deploy": "bash deploy.sh",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop kint-website",
    "pm2:restart": "pm2 restart kint-website",
    "pm2:logs": "pm2 logs kint-website",
    "postinstall": "prisma generate && node scripts/fix-permissions.js"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.16.0",
    "@tiptap/extension-image": "^3.18.0",
    "@tiptap/extension-link": "^3.18.0",
    "@tiptap/pm": "^3.18.0",
    "@tiptap/react": "^3.18.0",
    "@tiptap/starter-kit": "^3.18.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/mime": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.9.0",
    "cross-env": "^10.1.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "html-react-parser": "^5.2.13",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.563.0",
    "mime": "^4.1.0",
    "mysql2": "^3.15.1",
    "next": "^16.1.6",
    "next-auth": "5.0.0-beta.30",
    "next-intl": "^4.8.1",
    "prisma": "5.16.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "^0.34.5",
    "tsx": "^4.19.2",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.24",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.5.6",
    "shx": "^0.4.0",
    "tailwindcss": "^4.1.18"
  }
}
